        <servlet-class>com.shaveen.greensupermarket.OrderServlet</servlet-class>
    </servlet>
    <!-- Servlet Mappings -->
    <!-- (other servlet mappings remain unchanged) -->
    <servlet-mapping>
        <servlet-name>AddToWishListServlet</servlet-name>
        <url-pattern>/AddToWishListServlet</url-pattern>
        <servlet-name>DeleteAdminAccountServlet</servlet-name>
        <servlet-class>com.shaveen.greensupermarket.DeleteAdminAccountServlet</servlet-class>
